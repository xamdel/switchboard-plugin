{
  "name": "sixerr-plugin",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "exports": {
    "./openclaw": "./src/openclaw/index.ts",
    "./client": "./src/client/index.ts",
    "./setup": "./src/config/setup-programmatic.ts",
    "./start": "./src/start-programmatic.ts"
  },
  "bin": {
    "sixerr": "src/cli/cli.ts"
  },
  "scripts": {
    "setup": "tsx src/cli/cli.ts setup",
    "setup:headless": "tsx src/cli/cli.ts setup --non-interactive",
    "start": "tsx src/cli/cli.ts start",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "@coinbase/cdp-sdk": "^1.44.1",
    "jose": "^6.1.3",
    "open": "^11.0.0",
    "viem": "^2.45.3",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/ws": "^8.18.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
